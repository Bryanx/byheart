BEGIN TRANSACTION;
PRAGMA user_version = 3;
CREATE TABLE IF NOT EXISTS "Pile" (
	"id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	"languageCardFront"	TEXT NOT NULL,
	"languageCardBack"	TEXT NOT NULL,
	"color"	INTEGER,
	"name"	TEXT,
	"listIndex"	INTEGER NOT NULL DEFAULT -1
);
CREATE TABLE IF NOT EXISTS "card" (
	"id"	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	"question"	TEXT,
	"answer"	TEXT,
	"pile_id"	INTEGER NOT NULL,
	"listIndex"	INTEGER NOT NULL DEFAULT -1,
	"amountCorrect"	INTEGER NOT NULL DEFAULT 0,
	"amountFalse"	INTEGER NOT NULL DEFAULT 0,
	FOREIGN KEY("pile_id") REFERENCES "Pile"("id") ON UPDATE NO ACTION ON DELETE CASCADE
);
